<!doctype html>
<html lang="en">
<% include ../includes/head.ejs %>

<body>
  <% include ../includes/header.ejs %>

  <section class="section is-small">
    <div class="container">
      <% if (!locals.user && !locals.data) { %>
      <h1 class="title">Aucune donnée disponible</h1>
      <% } else { %>
      <h1 class="title">Voici les données que nous avons récupérées pour vous avec FranceConnect</h1>
      <% } %>

      <% if (locals.user) { %>
      <pre>
        <p><strong><i>Corps des données d'identité:</i></strong></p>
        <code id="json-output" class="json">
<%- JSON.stringify(locals.user, null, 2) %>
        </code>
      </pre>
      <% } %>
      <% if (locals.data) { %>
      <pre>
        <p><strong><i>Corps des données fournies par le fournisseur de données:</i></strong></p>
        <code id="json-data" class="json">
<%- JSON.stringify(locals.data, null, 2) %>
        </code>
      </pre>
      <% } %>
      <% if (locals.tracks) { %>
        <pre>
          <p><strong><i>Corps des données fournies par le fournisseur de données de traces:</i></strong></p>
          <code id="json-tracks" class="json">
  <%- JSON.stringify(locals.tracks, null, 2) %>
          </code>
        </pre>
        <% } %>
      <% if (locals.eIDASLevel) { %>
      <pre>
        <p><strong><i>Accréditation eIDAS :</i></strong></p>
        <code id="info-acr" class="json">
<%- locals.eIDASLevel %>
        </code>
      </pre>
      <% } %>
      <p class="has-text-centered">
        <a href="<%= locals.userLink %>">
          Voir les usagers disponibles
        </a>
      </p>
      <p class="has-text-centered">
        <a href="<%= locals.dataLink %>">
          Voir les données disponibles
        </a>
      </p>
    </div>
  </section>

  <section class="section is-small">
    <div class="container">
      <p class="has-text-centered">
        <a href="/" class="button is-large is-link">
          Revenir sur la page d'accueil
        </a>
      </p>
    </div>
  </section>

  <% include ../includes/footer.ejs %>
  <link rel="stylesheet" href="css/highlight.min.css">
  <script src="js/highlight.min.js"></script>
  <script>hljs.initHighlightingOnLoad();</script>
</body>

</html>
